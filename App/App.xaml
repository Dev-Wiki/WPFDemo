             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="../MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Resources/Images/images.xaml"/>
